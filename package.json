{
  "name": "angular-testing-boilerplate-framework",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "ng:update": "npm run ng -- update @angular/{cli,core}",
    "start": "ng serve --port 4200",
    "start:localdevelopment": "ng serve --port 4200 --configuration localdevelopment",
    "build": "ng build",
    "build:dev": "ng build --configuration development --base-href=./",
    "build:prod": "ng build --configuration production --base-href=./",
    "build:prod:correctrouting": "ng build --configuration production --base-href=/subfoldernameprojecthosting/",
    "dist": "npm run build:prod",
    "dist:default": "npm run dist && npm run zipscript",
    "setversion": "node ./setversion.js",
    "zipscript": "node ./zipscript.js",
    "ci:setversion": "npm run setversion --",
    "ci:build": "npm run build:prod:correctrouting",
    "ci:zip": "npm run zipscript --",
    "ci:test": "npm run test --",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:chrome": "ng test --browsers chrome",
    "test:chrome:headless": "ng test --karma-config karma.conf-chrome-headless.js",
    "test:chrome:locally": "ng test --karma-config karma.conf-chrome-locally.js",
    "test:msedge": "ng test --karma-config karma.conf-edge.js",
    "test:msedge:headless": "ng test --karma-config karma.conf-edge-headless.js",
    "test:msedge:locally": "ng test --karma-config karma.conf-edge-locally.js",
    "e2e": "ng e2e --configuration test-e2e",
    "e2e:headless:locally": "ng run angular-testing-boilerplate-framework:cypress-run --configuration test-e2e",
    "e2e:ci": "start-server-and-test start http://localhost:4200 cypress:run",
    "ci:start-server": "angular-http-server --path /dist/angular-testing-boilerplate-framework -p 4200",
    "ci:cypress:run": "start-server-and-test ci:start-server http://localhost:4200 cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "newcypressspecgen": "ng generate @cypress/schematic:e2e --name",
    "lint": "ng lint",
    "lint:format": "prettier -l \"src/**/*.{ts,scss,html}\"",
    "format": "prettier --write \"src/**/*.{ts,scss,html}\"",
    "prettier": "prettier --write \"src/app/**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "format:fix": "pretty-quick --staged",
    "precommit": "run-s format:fix lint",
    "pre-commit2": "run-s format:fix lint",
    "pre-commit": "run-s format:fix",
    "prepare": "husky install",
    "reinstall": "rm -rf ./node_modules && npm install",
    "ship": "npm run lint && npm run test && npm run e2e && npm run build",
    "test:cover": "rm -rf ./coverage && ng test --no-watch --code-coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.0",
    "@angular/cdk": "^13.3.2",
    "@angular/common": "^13.3.0",
    "@angular/compiler": "^13.3.0",
    "@angular/core": "^13.3.0",
    "@angular/forms": "^13.3.0",
    "@angular/platform-browser": "^13.3.0",
    "@angular/platform-browser-dynamic": "^13.3.0",
    "@angular/router": "^13.3.0",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.0",
    "@fortawesome/free-brands-svg-icons": "^6.1.0",
    "@fortawesome/free-regular-svg-icons": "^6.1.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.0",
    "@ngneat/transloco": "^4.0.0",
    "@ngneat/until-destroy": "^9.0.1",
    "bestzip": "^2.2.0",
    "bootstrap": "^5.1.3",
    "commander": "^9.1.0",
    "file-saver": "^2.0.5",
    "flatted": "^3.2.5",
    "inputmask": "^5.0.7",
    "lodash": "^4.17.21",
    "luxon": "^2.3.1",
    "primeflex": "^3.1.3",
    "primeicons": "^5.0.0",
    "primeng": "^13.3.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.1",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "^13.3.1",
    "@angular/compiler-cli": "^13.3.0",
    "@cypress/schematic": "^1.6.0",
    "@ngneat/spectator": "^10.0.1",
    "@testing-library/cypress": "^8.0.2",
    "@types/file-saver": "^2.0.5",
    "@types/inputmask": "^5.0.3",
    "@types/jasmine": "^3.10.0",
    "@types/lodash": "^4.14.181",
    "@types/luxon": "^2.3.1",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "angular-http-server": "^1.10.0",
    "autoprefixer": "^10.4.4",
    "cypress": "^9.5.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^38.1.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-edge-launcher": "^0.4.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-mocks": "^13.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "prettier": "^2.6.1",
    "prettier-eslint": "^13.0.0",
    "pretty-quick": "^3.1.3",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.23",
    "ts-node": "^10.7.0",
    "typescript": "~4.6.2"
  }
}
